<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
                                   
<hibernate-mapping>
    <class name="org.iocaste.documents.range.NumericRange" table="RANGE001">
        <id name="range" column="IDENT"/>
        <property name="current" column="CRRNT"/>
    </class>
    
    <class name="org.iocaste.documents.common.DocumentModel" table="DOCS001">
        <id name="name" column="DOCID"/>
        <property name="tableName" column="TNAME"/>
        <property name="className" column="CLASS"/>
        <set name="keys" inverse="true">
            <key column="DOCID"/>
            <one-to-many class="org.iocaste.documents.common.DocumentModelKey"/>
        </set>
        <set name="itens" inverse="true">
            <key column="DOCID"/>
            <one-to-many class="org.iocaste.documents.common.DocumentModelItem"/>
        </set>
    </class>
    
    <class name="org.iocaste.documents.common.DocumentModelItem" table="DOCS002">
        <id name="name" column="INAME"/>
        <many-to-one name="documentModel" column="DOCID" not-null="true"/>
        <many-to-one name="dataElement" column="ENAME" not-null="true"/>
        <property name="attributeName" column="ATTRB"/>
    </class>
    
    <class name="org.iocaste.documents.common.DataElement" table="DOCS003">
        <id name="name" column="ENAME"/>
        <property name="decimals" column="DECIM"/>
        <property name="length" column="LNGTH"/>
<!--        <property name="type" column="ETYPE"/> -->
    </class>
    
    <class name="org.iocaste.documents.common.DocumentModelKey" table="DOCS004">
        <id name="modelItem" column="INAME"/>
        <many-to-one name="model" column="DOCID" not-null="true"/>
    </class>
</hibernate-mapping>
                                   