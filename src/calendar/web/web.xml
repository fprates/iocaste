<?xml version="1.0" encoding="utf-8"?>
<web-app id="iocaste-calendar" xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
    <description>Calendar module</description>
    <display-name>Calendar module</display-name>
    
    <servlet>
        <servlet-name>iocaste_servlet</servlet-name>
        <servlet-class>org.iocaste.shell.common.IocasteServlet</servlet-class>
        <init-param>
            <param-name>form</param-name>
            <param-value>org.iocaste.calendar.Main</param-value>
        </init-param>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>iocaste_servlet</servlet-name>
        <url-pattern>/view.html</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>