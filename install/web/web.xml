<?xml version="1.0" encoding="utf-8"?>
<web-app id="iocaste_tests" xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
    <description>Iocaste Installer</description>
    <display-name>Iocaste Installer</display-name>
    
    <servlet>
        <servlet-name>install_servlet</servlet-name>
        <servlet-class>org.iocaste.install.Main</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>install_servlet</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>